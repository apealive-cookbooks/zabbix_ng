LogFile=<%= @logfile %>
PidFile=<%= @pidfile %>
DBHost=<%= @db_host %>
DBName=<%= @db_name %>
DBUser=<%= @db_user %>
<% if @db_password %>
DBPassword=<%= @db_password %>
<% end -%>

StartPollers=5
StartIPMIPollers=0
StartPollersUnreachable=1
StartTrappers=1
StartPingers=1
StartDiscoverers=1

HousekeepingFrequency=1
SenderFrequency=30
CacheSize=32M
Timeout=30
UnreachablePeriod=300
